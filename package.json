{
  "name": "pdf-to-anki-mcq",
  "description": "MCQ application for txt files generated by pdf to anki",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innocentwkc/pdf-to-anki-mcq.git"
  },
  "keywords": [
    "pdf-anki",
    "mcq"
  ],
  "author": "Innocent WK Chinyemba",
  "scripts": {
    "dev": "concurrently \"vite\" \"node server\"",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "start": "",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4005 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4005' http://localhost:4005 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pinia": "^2.1.7",
    "raw-socket": "^1.7.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cypress": "^13.3.1",
    "flowbite": "^2.2.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.1",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  },
  "bugs": {
    "url": "https://github.com/innocentwkc/baobabwatch/issues"
  },
  "homepage": "https://github.com/innocentwkc/baobabwatch#readme"
